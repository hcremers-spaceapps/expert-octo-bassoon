    "pluginVersion": "12.0.1",
    "targets": [
      {
        "datasource": {
          "type": "grafana-postgresql-datasource",
          "uid": "application-quality-db"
        },
        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT\n  elem ->> 'path' AS path,\n  elem ->> 'obj' AS object,\n  elem ->> 'message' AS message,\n  elem ->> 'message-id' AS code,\n  'L' || (elem ->> 'line') || 'C' || (elem ->> 'column') || CASE\n    WHEN elem ->> 'endLine' IS NOT NULL\n    AND elem ->> 'endColumn' IS NOT NULL THEN '-L' || (elem ->> 'endLine') || 'C' || (elem ->> 'endColumn')\n    ELSE ''\n  END AS location\nFROM\n  (\n    SELECT\n      jsonb_array_elements(output :: jsonb) AS elem\n    FROM\n      reports_admin \n    WHERE\n      name = 'pylint'\n      AND run_id = ${run_id}\n  ) AS sub;",
        "refId": "A",
        "sql": {
          "columns": [
            {
              "parameters": [],
              "type": "function"
            }
          ],
          "groupBy": [
            {
              "property": {
                "type": "string"
              },
              "type": "groupBy"
            }
          ],
          "limit": 50
        }
      }
    ],
    "title": "Pylint Report",
    "type": "table"
  }
