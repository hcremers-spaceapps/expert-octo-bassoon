        "editorMode": "code",
        "format": "table",
        "rawQuery": true,
        "rawSql": "SELECT\n  split_part(\n    elem ->> 'filename',\n    '/',\n    array_length(string_to_array(elem ->> 'filename', '/'), 1)\n  ) AS file,\n  elem ->> 'message' AS message,\n  elem ->> 'code' AS code,\n  (elem ->> 'cell') :: int AS cell,\n  'L' || (elem -> 'location' ->> 'row') || 'C' || (elem -> 'location' ->> 'column') || '-L' || (elem -> 'end_location' ->> 'row') || 'C' || (elem -> 'end_location' ->> 'column') AS location,\n  elem -> 'fix' ->> 'message' AS fix,\n  (elem -> 'fix' ->> 'applicability') = 'safe' AS safe\nFROM\n  (\n    SELECT\n      jsonb_array_elements(output :: jsonb) AS elem\n    FROM\n      reports_admin\n    WHERE\n      (\n        name = 'ruff'\n        AND run_id = ${run_id}\n      )\n  ) AS sub;",
